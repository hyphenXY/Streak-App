{
  "family": "streak-app-task",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "streak-app-container",
      "image": "362570697535.dkr.ecr.ap-south-1.amazonaws.com/streak-app:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DB_HOST",
          "value": "streak-app.cbg2g0akaveq.ap-south-1.rds.amazonaws.com"
        },
        {
          "name": "DB_USER",
          "value": "admin"
        },
        {
          "name": "DB_PASS",
          "value": "bIQtJnx3h0pED9LBGiEN"
        },
        {
          "name": "DB_NAME",
          "value": "streak-app"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/streak-app",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::362570697535:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::362570697535:role/ecsTaskExecutionRole"
}
